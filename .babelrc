{
  "env": {
    "development": {
      "presets": ["next/babel"],
      "plugins": [
        "inline-react-svg",
        "react-element-info",
        "macros",
        "babel-plugin-styled-components",
        ["module-resolver", {
          "root": ["./"],
          "alias": {
            "@helpers": "./helpers",
            "@api": "./api",
            "@components": "./components",
            "@styles": "./components/styles",
            "@theme": "./components/theme",
            "@static": "./static",
            "@queries": "./queries"
          }
        }]
      ]
    },
    "production": {
      "presets": ["next/babel"],
      "plugins": [
        "inline-react-svg",
        "macros",
        "transform-remove-console",
        "babel-plugin-styled-components",
        ["module-resolver", {
          "root": ["./"],
          "alias": {
            "@helpers": "./helpers",
            "@queries": "./queries",
            "@api": "./api",
            "@components": "./components",
            "@styles": "./components/styles",
            "@theme": "./components/theme",
            "@static": "./static"
          }
        }]
      ]
    }
  }
}
