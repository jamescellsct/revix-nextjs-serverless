{
  "name": "ac-react-simple-boilerplate",
  "version": "1.0.0",
  "description": "A light React SSR boilerplate built with React.js, styled-components, next.js, Apollo and GraphQL",
  "main": "server.js",
  "author": "Abdullah Ceylan <ben@abdullahceylan.com>",
  "license": "MIT",
  "scripts": {
    "dev": "node server.js",
    "dev:next": "next",
    "build": "export NODE_ENV=production; next build",
    "start": "export NODE_ENV=production; node server.js",
    "start:next": "next start -p $PORT",
    "heroku-postbuild": "npm run build"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@apollo/react-ssr": "^3.1.3",
    "@z1/lib-ui-box-tailwind": "^0.2.54",
    "@zeit/next-css": "^1.0.1",
    "apollo-boost": "^0.4.4",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link-http": "^1.5.16",
    "autoprefixer": "^9.6.5",
    "boxen": "^4.1.0",
    "chalk": "^2.4.2",
    "cross-env": "6.0.3",
    "express": "^4.16.4",
    "framer-motion": "^1.6.14",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "isomorphic-unfetch": "^3.0.0",
    "lodash": "^4.17.11",
    "next": "^9.1.1",
    "next-apollo": "^3.1.5",
    "next-with-apollo": "^4.3.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-apollo": "^3.1.3",
    "react-dom": "^16.10.2",
    "styled-components": "^4.1.3",
    "styled-reset": "^4.0.2",
    "styled-tools": "^1.7.0",
    "tailwindcss": "^1.1.2"
  },
  "devDependencies": {
    "babel-plugin-inline-react-svg": "^1.0.1",
    "babel-plugin-module-resolver": "^3.1.2",
    "babel-plugin-react-element-info": "^1.0.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react-hooks": "^2.1.2"
  }
}
